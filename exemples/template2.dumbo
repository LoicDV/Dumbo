<html>
   <head><title>{{ print nom; }}</title></head>
   <body>
      <h1>{{ print nom; }}</h1>
      {{
         i := 1;
         for nom in listephoto do
           print '<a href ="'.nom.'">'.nom.'</a>';
           i := i + 1 * 2 + 1 * 2;
         endfor;
      }}
   <br />
   Il y a {{ print i; }} photos dans l album "{{ print nom; }}".
   </body>
</html>
